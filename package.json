{
  "name": "fws-app",
  "version": "4.0.0",
  "description": "FWS Web App",
  "main": "index.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "pm2 start index.js --name fws-app -i 0",
    "stop": "pm2 kill",
    "build:css": "bin/build-css",
    "build": "bin/build",
    "lint": "standard",
    "unit-test": "bin/run-unit-tests",
    "pretest": "npm run docker:start-test-double-infrastructure",
    "test": "npm run lint & npm run unit-test",
    "posttest": "npm run docker:stop-test-double-infrastructure",
    "postinstall": "npm run build",
    "docker:start-infrastructure": "docker compose -f docker/infrastructure.yml up -d",
    "docker:start-test-double-infrastructure": "docker compose -f docker/infrastructure.yml up -d fws-app-redis",
    "docker:stop-infrastructure": "docker compose -f docker/infrastructure.yml down",
    "docker:stop-test-double-infrastructure": "docker compose -f docker/infrastructure.yml rm -f -s fws-app-redis"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/bell": "13.1.0",
    "@hapi/boom": "10.0.1",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/cookie": "12.0.1",
    "@hapi/hapi": "21.4.2",
    "@hapi/inert": "7.1.0",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.1.0",
    "govuk-frontend": "5.11.1",
    "hapi-pino": "12.1.0",
    "https-proxy-agent": "7.0.6",
    "joi": "18.0.1",
    "moment-timezone": "0.6.0",
    "nunjucks": "3.2.4",
    "pino": "9.9.0",
    "pino-pretty": "13.1.1"
  },
  "devDependencies": {
    "@hapi/code": "9.0.3",
    "@hapi/lab": "26.0.0",
    "blipp": "4.0.2",
    "proxyquire": "2.1.3",
    "sass": "1.91.0",
    "standard": "17.1.2"
  },
  "standard": {
    "ignore": [
      "server/dist",
      "service-down"
    ]
  }
}
