{
  "name": "fws-app",
  "version": "1.0.0",
  "description": "FWS Web App",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "pm2 start index.js --name fws-app -i 0",
    "stop": "pm2 kill",
    "build:css": "bin/build-css",
    "build": "bin/build",
    "lint": "standard",
    "unit-test": "lab -v -c -r console -o stdout -r lcov -o lcov.info",
    "test": "npm run lint & npm run unit-test",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/bell": "11.1.0",
    "@hapi/boom": "7.4.7",
    "@hapi/catbox-redis": "5.0.5",
    "@hapi/cookie": "10.1.2",
    "@hapi/good": "8.2.2",
    "@hapi/good-console": "8.1.2",
    "@hapi/good-squeeze": "5.2.1",
    "@hapi/hapi": "18.4.0",
    "@hapi/inert": "5.2.2",
    "@hapi/joi": "16.1.2",
    "@hapi/vision": "5.5.4",
    "@hapi/wreck": "15.1.0",
    "govuk-frontend": "3.2.0",
    "https-proxy-agent": "2.2.2",
    "moment-timezone": "0.5.26",
    "nunjucks": "3.2.0"
  },
  "devDependencies": {
    "@hapi/code": "6.0.0",
    "@hapi/lab": "20.2.2",
    "blipp": "4.0.1",
    "codeclimate-test-reporter": "0.5.1",
    "node-sass": "4.12.0",
    "standard": "14.3.1"
  }
}
